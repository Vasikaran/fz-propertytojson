{
  "name": "fz-propertytojson",
  "version": "1.0.0",
  "description": "convert propertyfile to json object",
  "main": "lib/index.js",
  "bin": {
    "propertToJson": "bin/build"
  },
  "scripts": {
    "clean": "rimraf lib coverage",
    "test": "jest --coverage --no-cache",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build": "npm run build:commonjs",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vimalceg/propertytojson.git"
  },
  "keywords": [
    "properties",
    "to",
    "json",
    "object"
  ],
  "author": "vimalesan.ceg@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vimalceg/propertytojson/issues"
  },
  "homepage": "https://github.com/vimalceg/propertytojson#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "rootDir": ".",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/library/"
    ],
    "unmockedModulePathPatterns": [
      "__tests__",
      "node_modules",
      ".*"
    ],
    "testPathDirs": [
      "<rootDir>/__tests__/"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html",
      "json-summary",
      "text"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(jsx|js|json|node)$",
    "transform": {
      ".*": "<rootDir>/__tests__/library/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/__tests__/library/setup.jsx"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jest": "^17.0.2",
    "babel-preset-react": "^6.5.0",
    "check-es3-syntax-cli": "^0.1.3",
    "cross-env": "^1.0.7",
    "expect": "^1.20.1",
    "fbjs": "^0.8.8",
    "jest": "^17.0.3",
    "jsdom": "^9.9.1",
    "object-assign": "^4.1.0",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.4.1",
    "rimraf": "^2.5.4"
  }
}
